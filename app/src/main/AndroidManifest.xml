<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.SonuzOvenUsers">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>




    <application
        android:allowBackup="true"
        android:usesCleartextTraffic="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.SonuzOvenUsers.ForgotPassword"></activity>


        <activity
            android:name="com.example.SonuzOvenUsers.ProcessDrawer"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.example.SonuzOvenUsers.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.SonuzOvenUsers.RegistrationActivity" />
        <activity android:name="com.example.SonuzOvenUsers.MainActivity" />
    </application>

</manifest>